<p style="color: green"><%= notice %></p>

<h1>Task list</h1>
<body><%= render_breadcrumbs :separator => ' / ' %></body>
<table id="task_list">
  <thead>
    <tr>
      <th>List</th>
      <th colspan="2">Actions</th>
    </tr>
  </thead>
  <tbody>
    <%= render @task_list %>
  </tbody>
</table>

<%= link_to "New task list", new_task_list_path %>
<%= button_to "Destroy this task list", @task_list, method: :delete %>
<%= link_to "Edit this task list", edit_task_list_path %>
<%= button_to "Back", task_lists_path %>